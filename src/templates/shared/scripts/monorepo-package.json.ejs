{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "private": true,
  "description": "Enterprise monorepo generated with The Architech",
  "author": "The Architech Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "<%= packageManager %>",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "type-check": "turbo run type-check",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "turbo": "^1.13.0",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "monorepo",
    "nextjs",
    "react",
    "typescript",
    "turborepo",
    "the-architech"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/<%= projectName %>.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/<%= projectName %>/issues"
  },
  "homepage": "https://github.com/your-username/<%= projectName %>#readme"
} 