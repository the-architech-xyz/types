{
  "name": "the-architech",
  "version": "0.1.0",
  "description": "Revolutionary AI-Powered Application Generator - Transforming weeks of work into minutes",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "architech": "./bin/architech.js",
    "the-architech": "./bin/architech.js"
  },
  "scripts": {
    "dev": "node bin/architech.js",
    "build": "npm run clean && npm run compile && npm run copy-bin",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "copy-bin": "cp -r bin dist/",
    "test": "echo 'Running tests...'",
    "start": "node bin/architech.js",
    "link": "npm link",
    "unlink": "npm unlink -g the-architech",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting...'",
    "format": "echo 'Formatting...'"
  },
  "keywords": [
    "cli",
    "generator",
    "ai",
    "nextjs",
    "react",
    "typescript",
    "shadcn",
    "tailwind",
    "architecture",
    "development",
    "productivity"
  ],
  "author": "The Architech Team",
  "license": "MIT",
  "preferGlobal": true,
  "dependencies": {
    "@types/ejs": "^3.1.5",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "ejs": "^3.1.10",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "src/",
    "dist/",
    "templates/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/the-architech/cli.git"
  },
  "bugs": {
    "url": "https://github.com/the-architech/cli/issues"
  },
  "homepage": "https://the-architech.dev",
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
