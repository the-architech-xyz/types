{
  "version": "1.0",
  "project": {
    "name": "Full Stack Test App",
    "framework": "nextjs",
    "description": "Complete test application with all adapters and integrations",
    "path": "./test-full-app"
  },
  "modules": [
    {
      "id": "framework/nextjs",
      "category": "framework",
      "version": "14.0.0",
      "parameters": {
        "typescript": true,
        "tailwind": true,
        "eslint": true,
        "appRouter": true,
        "srcDir": true,
        "importAlias": "@/*"
      },
      "features": []
    },
    {
      "id": "database/drizzle",
      "category": "database",
      "version": "0.29.0",
      "parameters": {
        "provider": "neon",
        "migrations": true,
        "studio": true,
        "databaseType": "postgresql"
      },
      "features": []
    },
    {
      "id": "auth/better-auth",
      "category": "auth",
      "version": "0.18.0",
      "parameters": {
        "providers": [
          "email",
          "google",
          "github"
        ],
        "session": "jwt",
        "csrf": true,
        "rateLimit": true
      },
      "features": []
    },
    {
      "id": "payment/stripe",
      "category": "payment",
      "version": "1.0.0",
      "parameters": {
        "mode": "test",
        "currencies": [
          "usd",
          "eur"
        ],
        "webhooks": true,
        "subscriptions": true
      },
      "features": []
    },
    {
      "id": "email/resend",
      "category": "email",
      "version": "1.0.0",
      "parameters": {
        "apiKey": "re_test_123",
        "templates": true,
        "analytics": true
      },
      "features": []
    },
    {
      "id": "observability/sentry",
      "category": "observability",
      "version": "1.0.0",
      "parameters": {
        "dsn": "https://test@sentry.io/123",
        "environment": "development",
        "performance": true,
        "profiling": true
      },
      "features": []
    },
    {
      "id": "state/zustand",
      "category": "state",
      "version": "1.0.0",
      "parameters": {
        "devtools": true,
        "persistence": true,
        "middleware": true
      },
      "features": []
    },
    {
      "id": "ui/shadcn-ui",
      "category": "ui",
      "version": "1.0.0",
      "parameters": {
        "theme": "default",
        "components": [
          "button",
          "input",
          "card",
          "dialog",
          "form"
        ],
        "darkMode": true
      },
      "features": []
    },
    {
      "id": "testing/vitest",
      "category": "testing",
      "version": "1.0.0",
      "parameters": {
        "coverage": true,
        "ui": true,
        "watch": true
      },
      "features": []
    },
    {
      "id": "deployment/docker",
      "category": "deployment",
      "version": "1.0.0",
      "parameters": {
        "multiStage": true,
        "production": true,
        "nginx": true
      },
      "features": []
    },
    {
      "id": "content/next-intl",
      "category": "content",
      "version": "1.0.0",
      "parameters": {
        "locales": [
          "en",
          "fr",
          "es"
        ],
        "defaultLocale": "en",
        "routing": true,
        "seo": true
      },
      "features": []
    },
    {
      "id": "blockchain/web3",
      "category": "blockchain",
      "version": "1.0.0",
      "parameters": {
        "networks": [
          "goerli",
          "polygon"
        ],
        "walletConnect": true,
        "contracts": []
      },
      "features": []
    }
  ],
  "options": {
    "skipInstall": true,
    "verbose": true
  }
}